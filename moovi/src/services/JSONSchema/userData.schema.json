{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "",
    "title": "Users",
    "description": "Data structure of users",
    "type": "object",
    "properties": {
        "userId": {
            "type": "string"
        },
        "userName": {
            "type": "string"
        },
        "userEmail": {
            "type": "string",
            "format": "email"
        },
        "userPassword": {
            "type": "string"
        },
        "userAvatar": {
            "description": "img src of user avatar",
            "type": "string"
        }
    }
}